<idea-plugin version="2">
	<id>org.consulo.jruby.rails</id>
	<name>JRuby on Rails</name>
	<version>1.0</version>

	<depends>org.consulo.ruby.rails</depends>

	<extensions defaultExtensionNs="com.intellij">
		<moduleExtensionProvider key="jruby-on-rails"
								 parentKey="jruby"
								 icon="/org/jetbrains/plugins/ruby/rails/rails_small.png"
								 name="Rails"
								 immutableClass="org.consulo.jruby.rails.module.extension.JRubyOnRailsModuleExtension"
								 mutableClass="org.consulo.jruby.rails.module.extension.JRubyOnRailsMutableModuleExtension" />
	</extensions>
</idea-plugin>