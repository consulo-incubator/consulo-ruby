<configuration default="false" name="Server:radiant" type="RubyRunConfigurationType" factoryName="Server">
    <RAILS_SERVER_CONFIG_SETTINGS_ID NAME="SCRIPT_PATH" VALUE="$PROJECT_DIR$/script/server"/>
    <RAILS_SERVER_CONFIG_SETTINGS_ID NAME="SCRIPT_ARGS" VALUE=""/>
    <RAILS_SERVER_CONFIG_SETTINGS_ID NAME="RUBY_ARGS"
                                     VALUE="-e STDOUT.sync=true;STDERR.sync=true;load($0=ARGV.shift)"/>
    <RAILS_SERVER_CONFIG_SETTINGS_ID NAME="WORK DIR" VALUE="$PROJECT_DIR$"/>
    <RAILS_SERVER_CONFIG_SETTINGS_ID NAME="MODULE_NAME" VALUE="radiant"/>
    <RAILS_SERVER_CONFIG_SETTINGS_ID NAME="SHOULD_USE_SDK" VALUE="false"/>
    <RAILS_SERVER_CONFIG_SETTINGS_ID NAME="ALTERN_SDK_NAME" VALUE=""/>
    <RAILS_SERVER_CONFIG_SETTINGS_ID NAME="PORT" VALUE="3000"/>
    <RAILS_SERVER_CONFIG_SETTINGS_ID NAME="IP" VALUE="0.0.0.0"/>
    <RAILS_SERVER_CONFIG_SETTINGS_ID NAME="CHOOSE_MANUALLY" VALUE="true"/>
    <method>
        <option name="Make" value="true"/>
    </method>
    <RAILS_SERVER_CONFIG_SETTINGS_ID NAME="SERVER_TYPE" VALUE="DEFAULT"/>
</configuration>